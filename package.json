{
  "name": "ember-scroll-modifiers",
  "version": "6.0.0",
  "description": "Scroll-based Modifiers for Ember Applications",
  "keywords": [
    "ember-addon",
    "modifier",
    "modifiers",
    "scroll"
  ],
  "repository": "https://github.com/elwayman02/ember-scroll-modifiers",
  "license": "MIT",
  "author": "Jordan Hawker <hawker.jordan@gmail.com> (https://www.JordanHawker.com)",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "concurrently \"npm:lint:*(!fix|hbs)\" --names \"lint:\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "concurrently \"npm:lint\" \"npm:test:*\" --names \"lint,test:\"",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "dependencies": {
    "ember-auto-import": "^2.6.3",
    "ember-cli-babel": "^7.26.11",
    "ember-cli-htmlbars": "^6.2.0",
    "ember-modifier": "^4.1.0",
    "intersection-observer-admin": "^0.3.3"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.21.3",
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.9.3",
    "@embroider/test-setup": "^2.1.1",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "broccoli-asset-rev": "^3.0.0",
    "concurrently": "^8.0.1",
    "ember-cli": "~4.12.1",
    "ember-cli-dependency-checker": "^3.3.1",
    "ember-cli-deprecation-workflow": "^2.0.0",
    "ember-cli-fastboot": "^4.1.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sass": "^11.0.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-data": "~4.9.1",
    "ember-fetch": "^8.1.2",
    "ember-load-initializers": "^2.1.2",
    "ember-page-title": "^7.0.0",
    "ember-qunit": "^6.2.0",
    "ember-resolver": "^9.0.1",
    "ember-sinon-qunit": "^6.0.0",
    "ember-source": "~4.12.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^5.7.3",
    "ember-try": "^2.0.0",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-ember": "^11.5.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-qunit": "^7.3.4",
    "field-guide": "^2.4.0",
    "field-guide-default-template": "^3.0.0",
    "loader.js": "^4.7.0",
    "prember": "^1.1.1",
    "prettier": "^2.8.8",
    "qunit": "^2.19.4",
    "qunit-dom": "^2.0.0",
    "release-it": "^15.10.3",
    "release-it-lerna-changelog": "^5.0.0",
    "sass": "^1.62.1",
    "webpack": "^5.82.0"
  },
  "fastbootDependencies": [
    "crypto"
  ],
  "peerDependencies": {
    "ember-source": "^3.28.0 || ^4.0.0"
  },
  "engines": {
    "node": "14.* || 16.* || >= 18"
  },
  "volta": {
    "node": "14.21.1",
    "yarn": "1.22.19"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://ember-scroll-modifiers.jhawk.co/"
  },
  "homepage": "https://ember-scroll-modifiers.jhawk.co/",
  "release-it": {
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md",
        "launchEditor": false
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "tokenRef": "GITHUB_AUTH"
    }
  }
}
