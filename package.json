{
  "name": "workspace-root",
  "version": "7.3.0",
  "private": true,
  "repository": "",
  "license": "MIT",
  "author": "",
  "scripts": {
    "build": "pnpm --filter ember-scroll-modifiers run build",
    "lint": "pnpm --filter ember-scroll-modifiers run lint",
    "lint:fix": "pnpm --filter ember-scroll-modifiers run lint:fix",
    "prepare": "pnpm build",
    "start": "concurrently 'pnpm:start:*' --restart-after 5000 --prefix-colors cyan,white,yellow",
    "start:addon": "pnpm --filter ember-scroll-modifiers ember-scroll-modifiers run start",
    "start:test-app": "pnpm --filter ember-scroll-modifiers test-app run start",
    "test": "pnpm --filter ember-scroll-modifiers run test"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "packageManager": "pnpm@9.5.0"
}
